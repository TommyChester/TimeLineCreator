<p id="notice"><%= notice %></p>

<h1>Relative Time Spaces</h1>

<table>
  <thead>
    <tr>
      <th>Epoch</th>
      <th>Epochtime</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @relative_time_spaces.each do |relative_time_space| %>
      <tr>
        <td><%= relative_time_space.epoch %></td>
        <td><%= relative_time_space.epochtime %></td>
        <td><%= link_to 'Show', relative_time_space %></td>
        <td><%= link_to 'Edit', edit_relative_time_space_path(relative_time_space) %></td>
        <td><%= link_to 'Destroy', relative_time_space, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Relative Time Space', new_relative_time_space_path %>
